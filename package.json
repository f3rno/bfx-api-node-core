{
  "name": "bfx-api-node-core",
  "version": "1.1.9",
  "description": "Core Bitfinex Node API",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "lint": "eslint examples/ test/ lib/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 85 --branches 70 --functions 80 --statements 80 --reporter=lcov --reporter=html mocha -b --recursive",
    "docs": "npm run manager_docs && npm run ws2_func_docs",
    "manager_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/manager.js > docs/manager_docs.md",
    "ws2_func_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ws2/*.js lib/ws2/orders/*.js lib/ws2/flags/*.js > docs/ws2_funcs.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-api-node-core.git"
  },
  "keywords": [
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-api-node-core/issues"
  },
  "homepage": "http://bitfinexcom.github.io/bfx-api-node-core/",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "standard": "^14.1.0"
  },
  "dependencies": {
    "bfx-api-node-models": "^1.1.9",
    "bfx-api-node-rest": "^3.0.4",
    "bfx-api-node-util": "^1.0.8",
    "bfx-api-node-ws1": "^1.0.3",
    "bluebird": "^3.7.2",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "p-iteration": "^1.1.8",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.2.2"
  }
}
